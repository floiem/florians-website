{
  "name": "florians-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "--- For development ------------------------": "",
    "dev": "vite",
    "dev:vercel": "vercel dev",
    "start": "vite build && vite preview",
    "--- For production -------------------------": "",
    "vercel-build": "npm run generate && vite build",
    "--- Single commands ------------------------": "",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .vite",
    "generate": "node ./build/generateOpengraph.js",
    "--- Not in use, but necessary to include ---": "",
    "build": "paraglide-js compile --project ./project.inlang.json --namespace florians-site && npm run generate && vite build"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@egjs/flicking-plugins": "^4.7.1",
    "@egjs/preact-flicking": "^4.11.0",
    "@preact/preset-vite": "^2.3.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vercel/analytics": "^1.1.1",
    "@vercel/node": "^3.0.7",
    "cross-fetch": "^4.0.0",
    "firebase": "^10.5.0",
    "marked": "^9.0.3",
    "node-fetch": "^3.3.2",
    "preact": "^10.10.0",
    "preact-render-to-string": "^5.2.1",
    "pureimage": "^0.4.13",
    "signature_pad": "^4.1.6",
    "typescript": "^5.2.2",
    "vike": "^0.4.144",
    "vite": "^4.0.3",
    "@inlang/paraglide-js": "1.0.0-prerelease.5"
  },
  "devDependencies": {
    "@iconify-json/eva": "^1.1.7",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "unplugin-icons": "0.17.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  }
}
